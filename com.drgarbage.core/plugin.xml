<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         id="com.drgarbage.bytecodevisualizer.plugin.preferencePages"
         point="org.eclipse.ui.preferencePages">
      <page
            category="com.drgarbage.MainPage"
            class="com.drgarbage.core.preferences.GraphColorsPreferencePage"
            id="com.drgarbage.bytecodevisualizer.preferences.FigurePreferences"
            name="%page.Graph_Colors"/>
      <page
            class="com.drgarbage.core.preferences.DrGarbagePreferencePage"
            id="com.drgarbage.MainPage"
            name="%category.Dr_Garbage">
      </page>
      <page
            category="com.drgarbage.MainPage"
            class="com.drgarbage.core.preferences.GraphPanelLocationPreferencePage"
            id="com.drgarbage.bytecodevisualizer.ControlFlowGraphPanel"
            name="%prefPage_Graph_Panel_Location">
      </page>
   </extension>
   <extension
         id="com.drgarbage.bytecodevisualizer.plugin.preferences"
         point="org.eclipse.core.runtime.preferences">
      <initializer class="com.drgarbage.core.preferences.PreferenceInitializer"/>
   </extension>
