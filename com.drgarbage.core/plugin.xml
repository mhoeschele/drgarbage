   <extension
         point="org.eclipse.ui.views">
      <category
            name="%category.Dr_Garbage"
            id="com.drgabage.bytecodevisualizer.category.controlflowgraph_view">
      </category>
      <view
            allowMultiple="false"
            category="com.drgabage.bytecodevisualizer.category.controlflowgraph_view"
            class="com.drgarbage.core.views.ControlFlowGraphView"
            icon="$nl$/com/drgarbage/core/img/bytecode-viewer-16x16.png"
            id="com.drgabage.bytecodevisualizer.view.controlflowgraph_view"
            name="%ControlFlowGraphView_title">
      </view>
      <stickyView
            closeable="true"
            id="com.drgabage.bytecodevisualizer.view.controlflowgraph_view"
            location="RIGHT"
            moveable="true">
      </stickyView>
   </extension>
